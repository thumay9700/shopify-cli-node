{
  "name": "shopify-cli-node",
  "description": "Shopify CLI for Node.js development",
  "version": "0.0.0",
  "author": "@thumay9700",
  "bin": {
    "shopify-cli": "./bin/run.js"
  },
  "bugs": "https://github.com/thumay9700/shopify-cli-node/issues",
  "dependencies": {
    "@oclif/core": "^4",
    "@oclif/plugin-help": "^6",
    "@oclif/plugin-plugins": "^5",
    "@types/inquirer": "^9.0.9",
    "axios": "^1.11.0",
    "dotenv": "^17.2.1",
    "fs-extra": "^11.3.1",
    "inquirer": "^12.9.0",
    "joi": "^18.0.0",
    "js-yaml": "^4.1.0",
    "socks-proxy-agent": "^8.0.5"
  },
  "devDependencies": {
    "@eslint/compat": "^1",
    "@jest/globals": "^30.0.5",
    "@oclif/prettier-config": "^0.2.1",
    "@oclif/test": "^4",
    "@types/chai": "^4",
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^30.0.0",
    "@types/js-yaml": "^4.0.9",
    "@types/mocha": "^10",
    "@types/node": "^18.19.121",
    "chai": "^4",
    "eslint": "^9",
    "eslint-config-oclif": "^6",
    "eslint-config-prettier": "^10",
    "jest": "^30.0.5",
    "mocha": "^10",
    "oclif": "^4",
    "shx": "^0.3.3",
    "ts-jest": "^29.4.1",
    "ts-node": "^10",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "./bin",
    "./dist",
    "./oclif.manifest.json"
  ],
  "homepage": "https://github.com/thumay9700/shopify-cli-node",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "oclif": {
    "bin": "shopify-cli",
    "dirname": "shopify-cli",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " ",
    "topics": {
      "hello": {
        "description": "Say hello to the world and others"
      }
    }
  },
  "repository": "thumay9700/shopify-cli-node",
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "npm run lint",
    "prepack": "oclif manifest && oclif readme",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "./scripts/test-ci.sh",
    "version": "oclif readme && git add README.md",
    "release:patch": "npm version patch && git push && git push --tags",
    "release:minor": "npm version minor && git push && git push --tags",
    "release:major": "npm version major && git push && git push --tags",
    "release:prerelease": "npm version prerelease --preid=alpha && git push && git push --tags"
  },
  "types": "dist/index.d.ts"
}
